"vertical-slider":{ 
  "props":{ 
    "arrayOfImages":["assets/home/nissan-aspot.jpg",
      "https://sydneywidecashforcars.com.au/wp-content/uploads/2015/05/Cash-for-Nissan-Cars-1024x430.jpg",
      "https://www.nissan-cdn.net/content/dam/Nissan/nissan_latin_america/HB/next/x-gear/Home_Desktop_3840x1575.jpg.ximg.full.hero.jpg"], 
    "imageHeight":"500px", 
    "imageWidth":"100%", 
    "blockClass":"verticalSlider", 
    "settings" :{ 
      "infinite": true, 
      "speed": 1000, 
      "slidesToShow": 1, 
      "slidesToScroll":-1, 
      "autoplay":true, 
      "pauseOnHover":false,
      "autoplaySpeed":3000
    
    }
