        "noFollow": false,
        "tagTitle": "Checkout",
        "text": "Checkout"
      }
    }
  },

  "menu-item#account-register": {
    "props": {
      "blockClass": "sub-header",
      "id": "menu-item-account-register",
      "type": "custom",
      "iconId": null,
      "highlight": false,
      "itemProps": {
        "type": "internal",
