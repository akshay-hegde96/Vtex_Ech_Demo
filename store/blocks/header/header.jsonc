        "noFollow": false,
        "tagTitle": "Register",
        "text": "Register"
      }
    }
  },
  "menu-item#account-login": {
    "props": {
      "blockClass": "sub-header",
      "id": "menu-item-account-login",
      "type": "custom",
      "iconId": null,
      "highlight": false,
      "itemProps": {
        "type": "internal",
        "href": "/login",
        "noFollow": false,
        "tagTitle": "Login",
        "text": "Login"
      }
    }
  },
  //----------------------

  "menu-item#static-category-electronics-mobile": {
